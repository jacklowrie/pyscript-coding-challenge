{
  "name": "python-coding-challenge",
  "type": "module",
  "version": "0.0.1",
  "packageManager": "pnpm@10.26.0",
  "scripts": {
    "dev": "pnpm run minify-pyscript && astro dev",
    "build": "pnpm run minify-pyscript && astro build",
    "preview": "astro preview",
    "astro": "astro",
    "format": "prettier --write",
    "format:check": "prettier --check",
    "minify-pyscript": "mkdir -p public && cp -R srcpy/*.py public/ && uv run pyminify public/ --in-place --remove-literal-statements"
  },
  "dependencies": {
    "@iconify-json/fa7-brands": "^1.2.1",
    "ace-builds": "^1.43.5",
    "astro": "^5.16.6",
    "astro-icon": "^1.1.5",
    "bulma": "^1.0.4",
    "typescript": "^5.9.3"
  },
  "volta": {
    "node": "24.12.0",
    "pnpm": "10.26.0"
  },
  "devDependencies": {
    "@astrojs/check": "^0.9.6",
    "@types/ace": "^0.0.52",
    "prettier": "3.7.4",
    "prettier-plugin-astro": "0.14.1"
  }
}
